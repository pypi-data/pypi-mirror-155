test_values_iter:
  # `test_getters.py` thoroughly tests cases where there is only a single 
  # getter.  The tests here therefore focus on cases where there are multiple 
  # getters.

  -
    id: layers-1x
    getters:
      - Value(1)
    expected:
      - 1
    log:
      - got hard-coded value: 1
  -
    id: layers-2x
    getters:
      - Value(1)
      - Value(2)
    expected:
      - 1
      - 2
    log:
      - got hard-coded value: 1
      - got hard-coded value: 2
  -
    id: layers-repeat
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'x')
      - Key(DummyConfig, 'x')
    expected:
      - 1
      - 1
    log:
      - queried DummyConfig():
      - found 'x': 1
      - queried DummyConfig():
      - found 'x': 1
  -
    id: layers-repeat
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >         yield DictLayer({'x': 2})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'x')
      - Key(DummyConfig, 'x')
    expected:
      - 1
      - 2
      - 1
      - 2
    log:
      - queried DummyConfig():
      - found 'x': 1
      - found 'x': 2
      - queried DummyConfig():
      - found 'x': 1
      - found 'x': 2
  -
    id: layers-order
    obj:
      > class DummyConfigA(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyConfigB(Config):
      >     def load(self):
      >         yield DictLayer({'x': 2})
      >
      > class DummyObj:
      >     __config__ = [DummyConfigA, DummyConfigB]
    getters:
      - Key(DummyConfigA, 'x')
      - Key(DummyConfigB, 'x')
    expected:
      - 1
      - 2
    log:
      - queried DummyConfigA():
      - found 'x': 1
      - queried DummyConfigB():
      - found 'x': 2
  -
    id: layers-order
    obj:
      > class DummyConfigA(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyConfigB(Config):
      >     def load(self):
      >         yield DictLayer({'x': 2})
      >
      > class DummyObj:
      >     __config__ = [DummyConfigA, DummyConfigB]
    getters:
      - Key(DummyConfigB, 'x')
      - Key(DummyConfigA, 'x')
    expected:
      - 2
      - 1
    log:
      - queried DummyConfigB():
      - found 'x': 2
      - queried DummyConfigA():
      - found 'x': 1
  -
    id: layers-missing
    obj:
      > class DummyConfigA(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyConfigB(Config):
      >     def load(self):
      >         yield DictLayer({'x': 2})
      >
      > class DummyObj:
      >     __config__ = [DummyConfigA, DummyConfigB]
    getters:
      - Key(DummyConfigA, 'x')
      - Key(DummyConfigB, 'y')
    expected:
      - 1
    log:
      - queried DummyConfigA():
      - found 'x': 1
      - queried DummyConfigB():
      - did not find 'y' in {'x': 2}
  -
    id: layers-missing
    obj:
      > class DummyConfigA(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyConfigB(Config):
      >     def load(self):
      >         yield DictLayer({'x': 2})
      >
      > class DummyObj:
      >     __config__ = [DummyConfigA, DummyConfigB]
    getters:
      - Key(DummyConfigA, 'y')
      - Key(DummyConfigB, 'x')
    expected:
      - 2
    log:
      - queried DummyConfigA():
      - did not find 'y' in {'x': 1}
      - queried DummyConfigB():
      - found 'x': 2
  -
    id: layers-err
    getters:
    expected:
    log:
      - nowhere to look for values
      - did you mean to provide a default?
  -
    id: layers-err
    obj:
      > class DummyConfig(Config):
      >     autoload = False
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'x')
    expected:
    log:
      - skipped DummyConfig(): not loaded
      - did you mean to call `byoc.load()`?
      - did you mean to provide a default?
  -
    id: layers-err
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield from []
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'x')
    expected:
    log:
      - skipped DummyConfig(): loaded, but no layers
      - did you mean to provide a default?
  -
    id: layers-err
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'y')
    expected:
    log:
      - queried DummyConfig():
      - did not find 'y' in {'x': 1}
      - did you mean to provide a default?
  -
    id: cast
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >         yield DictLayer({'x': 2})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'x', cast=lambda x: x + 1)
    expected:
      - 2
      - 3
    log:
      - queried DummyConfig():
      - found 'x': 1
      - found 'x': 2
  -
    id: cast
    obj:
      > class DummyConfigA(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyConfigB(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyObj:
      >     __config__ = [DummyConfigA, DummyConfigB]
    getters:
      - Key(DummyConfigA, 'x', cast=lambda x: x + 1)
      - Key(DummyConfigB, 'x', cast=lambda x: x + 2)
    expected:
      - 2
      - 3
    log:
      - queried DummyConfigA():
      - found 'x': 1
      - queried DummyConfigB():
      - found 'x': 1
  -
    id: default
    getters:
    default: 0
    expected:
      - 0
    log:
      - nowhere to look for values
      - got default value: 0
  -
    id: default
    getters:
      - Value(1)
    default: 0
    expected:
      - 1
      - 0
    log:
      - got hard-coded value: 1
      - got default value: 0
  -
    id: default
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'x')
    default: 0
    expected:
      - 1
      - 0
    log:
      - queried DummyConfig():
      - found 'x': 1
      - got default value: 0
  -
    id: default
    obj:
      > class DummyConfig(Config):
      >     def load(self):
      >         yield DictLayer({'x': 1})
      >
      > class DummyObj:
      >     __config__ = [DummyConfig]
    getters:
      - Key(DummyConfig, 'y')
    default: 0
    expected:
      - 0
    log:
      - queried DummyConfig():
      - did not find 'y' in {'x': 1}
      - got default value: 0


test_pick_functions:
  -
    id: first-empty
    pick_func: byoc.first
    values_iter: []
    error: NoValueFound
  -
    id: first-1
    pick_func: byoc.first
    values_iter:
      - 1, 2
    expected:
      value: 1
      meta: 2
  -
    id: first-2
    pick_func: byoc.first
    values_iter:
      - 1, 2
      - 3, 4
    expected:
      value: 1
      meta: 2
  -
    id: list-empty
    pick_func: byoc.list
    values_iter: []
    expected:
      value: []
      meta: []
  -
    id: list-1
    pick_func: byoc.list
    values_iter:
      - 1, 2
    expected:
      value: [1]
      meta: [2]
  -
    id: list-2
    pick_func: byoc.list
    values_iter:
      - 1, 2
      - 3, 4
    expected:
      value: [1, 3]
      meta: [2, 4]
  -
    id: merge-dicts-empty
    pick_func: byoc.merge_dicts
    values_iter: []
    expected:
      value: {}
      meta: {}
  -
    id: merge-dicts-keep-all
    pick_func: byoc.merge_dicts
    values_iter:
      - {'a': 1}, 2
      - {'b': 3}, 4
    expected:
      value: {'a': 1, 'b': 3}
      meta: {'a': 2, 'b': 4}
  -
    id: merge-dicts-keep-first
    pick_func: byoc.merge_dicts
    values_iter:
      - {'a': 1}, 2
      - {'a': 3}, 4
    expected:
      value: {'a': 1}
      meta: {'a': 2}
  -
    id: merge-dicts-keep-last
    pick_func: byoc.merge_dicts(keep_last=True)
    values_iter:
      - {'a': 1}, 2
      - {'a': 3}, 4
    expected:
      value: {'a': 3}
      meta: {'a': 4}







appid: {{ appid }}
services:
  wsgi:
    app: {{ name }}.app:app
  grpc:
    app: {{ name }}.service:service

consumers:
  - broker: amqp
    type: amqp
    concurrency: 1
    queues:
      - name: amqp01
        handler: {{ name }}.mq.amqp.consumer:handle_message
  - broker: kafka
    type: kafka
    topics:
      - name: topic01
        handler: {{ name }}.mq.kafka.consumer:handle_message
        group: T20220430
        concurrency: 3

log_lv: debug
#log_dir: /ssd

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/neat/:ApplyTrainedModelsContainer> a sh:NodeShape ;
    sh:closed true ;
    sh:description "A container with multiple classifiers in it." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/neat/:ApplyTrainedModel> ;
            sh:description "Models that need to be used for link prediction." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:models> ] ;
    sh:targetClass <https://w3id.org/neat/:ApplyTrainedModelsContainer> .

<https://w3id.org/neat/:ClassifierContainer> a sh:NodeShape ;
    sh:closed true ;
    sh:description "A container with multiple classifiers in it." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/neat/:Classifier> ;
            sh:description "Classifier details." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:classifiers> ] ;
    sh:targetClass <https://w3id.org/neat/:ClassifierContainer> .

<https://w3id.org/neat/:EmbeddingsConfig> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Embeddings configuration." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Embeddings history file name." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:history_filename> ],
        [ sh:description "Embeddings file name." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:filename> ],
        [ sh:class <https://w3id.org/neat/:NodeEmbeddingsParams> ;
            sh:description "Node embeddings parameters." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:node_embeddings_params> ],
        [ sh:description "File name for the TSNE plot." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path <https://w3id.org/neat/:tsne_filename> ] ;
    sh:targetClass <https://w3id.org/neat/:EmbeddingsConfig> .

<https://w3id.org/neat/:LayerParams> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Parameters for each layer of a neural network." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Activation layer type" ;
            sh:in ( "linear" "relu" "sigmoid" "tanh" "softmax" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:activation> ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:rate> ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:units> ] ;
    sh:targetClass <https://w3id.org/neat/:LayerParams> .

<https://w3id.org/neat/:NeatConfiguration> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Root class for the schema." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/neat/:GraphDataConfiguration> ;
            sh:description "Configuration for graph data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:graph_data> ] ;
    sh:targetClass <https://w3id.org/neat/:NeatConfiguration> .

<https://w3id.org/neat/:Target> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Path for output to be saved." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "File path for saving results." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:target_path> ] ;
    sh:targetClass <https://w3id.org/neat/:Target> .

<https://w3id.org/neat/:Upload> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Configuration for uploading to Amazon S3 bucket." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Extra keyword arguments (**kwargs)." ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:extra_args> ],
        [ sh:description "Bucket path." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:s3_bucket_dir> ],
        [ sh:description "Bucket name." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:s3_bucket> ] ;
    sh:targetClass <https://w3id.org/neat/:Upload> .

<https://w3id.org/neat/:ApplyTrainedModel> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Trained model used for prediction." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Key of the model to be used." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:model_id> ],
        [ sh:description "File path for saving results." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path <https://w3id.org/neat/:outfile> ],
        [ sh:class <https://w3id.org/neat/:NodeType> ;
            sh:description "Type of nodes." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:node_types> ],
        [ sh:description "Cutoff filter." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:cutoff> ] ;
    sh:targetClass <https://w3id.org/neat/:ApplyTrainedModel> .

<https://w3id.org/neat/:Classifier> a sh:NodeShape ;
    sh:closed true ;
    sh:description "ML classifiers to be trained-tested-validated and applied for predictions." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Key to identify the classifier and associated parameters." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:classifier_id> ],
        [ sh:description "Type of classifier." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:classifier_type> ],
        [ sh:class <https://w3id.org/neat/:ClassifierParams> ;
            sh:description "Parameters to be passed for building classifier." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path <https://w3id.org/neat/:parameters> ],
        [ sh:description "Edge method." ;
            sh:in ( "Average" "Hadamard" "Sum" "L1" "L2" "AbsoluteL1" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path <https://w3id.org/neat/:edge_method> ],
        [ sh:description "File name for saving history." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path <https://w3id.org/neat/:history_filename> ],
        [ sh:description "Name of the classifier." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:classifier_name> ],
        [ sh:description "Fie path for saving output." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path <https://w3id.org/neat/:outfile> ] ;
    sh:targetClass <https://w3id.org/neat/:Classifier> .

<https://w3id.org/neat/:ClassifierCallback> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Utilities called at certain points during model training." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Quantity to be monitored." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:monitor> ],
        [ sh:description "Type of callback." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:type> ],
        [ sh:description "One of {\"auto\", \"min\", \"max\"}. In min mode, training will stop when the quantity monitored has stopped decreasing; in \"max\" mode it will stop when the quantity monitored has stopped increasing; in \"auto\" mode, the direction is automatically inferred from the name of the monitored quantity." ;
            sh:in ( "auto" "min" "max" ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path <https://w3id.org/neat/:mode> ],
        [ sh:description "Number of epochs with no improvement after which training will be stopped." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:patience> ],
        [ sh:description "Minimum change in the monitored quantity to qualify as an improvement, i.e. an absolute change of less than min_delta, will count as no improvement." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path <https://w3id.org/neat/:min_delta> ],
        [ sh:description "Verbosity mode, 0 or 1. Mode 0 is silent, and mode 1 displays messages when the callback takes an action." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path <https://w3id.org/neat/:verbose> ] ;
    sh:targetClass <https://w3id.org/neat/:ClassifierCallback> .

<https://w3id.org/neat/:ClassifierCallbackContainer> a sh:NodeShape ;
    sh:closed true ;
    sh:description "A container of classifier callbacks." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/neat/:ClassifierCallback> ;
            sh:description "Callbacks." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:callbacks> ] ;
    sh:targetClass <https://w3id.org/neat/:ClassifierCallbackContainer> .

<https://w3id.org/neat/:ClassifierFitParams> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Paramters for fitting a classifier." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/neat/:ClassifierCallbackContainer> ;
            sh:description "Container of callbacks." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat/:callbacks_list> ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat/:batch_size> ],
        [ sh:description "Number of epochs to run for training." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat/:epochs> ] ;
    sh:targetClass <https://w3id.org/neat/:ClassifierFitParams> .

